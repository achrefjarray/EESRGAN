{
    "name": "ImagePatchClassifier_TinyNet",
    "n_gpu": 0,

    "arch": {
        "type": "ImagePatchClassifier",
        "args": {}
    },
    "data_loader": {
        "type": "COWCDataLoader",
        "args":{
            "data_dir": "/kaggle/input/eesrgan-model-data/eesrganmdels+dataset/dataset/",
            "batch_size": 1,
            "shuffle": true,
            "validation_split": 0.1,
            "num_workers": 2
        }
    },
    "optimizer": {
        "type": "SGD",
        "args":{
            "lr": 0.0001,
            "weight_decay": 0,
            "momentum": 0.9
        }
    },
    "loss": "cross_entropy",
    "metrics": [
        "accuracy"
    ],
    "lr_scheduler": {
        "type": "StepLR",
        "args": {
            "step_size": 50,
            "gamma": 0.1
        }
    },
    "trainer": {
        "epochs": 2,

        "save_dir": "saved/",
        "save_period": 1,
        "verbosity": 2,

        "monitor": "min val_loss",
        "early_stop": 10,

        "tensorboard": false
    }
}
